<QuestionModule xmlns="sqbl:1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="NEW_MODULE" version="1"
    xsi:schemaLocation="sqbl:1 ../../Schemas/sqbl.xsd">
    <WordSubstitutions>
        <WordSub name="sex_pronoun">
            <TextComponent xml:lang="en">
                <Default>their</Default>
            </TextComponent>
            <Condition>
                <ValueOf question="Sex" is="equal_to">F</ValueOf>
                <ResultString>
                    <TextComponent xml:lang="en">her</TextComponent>
                </ResultString>
            </Condition>
            <Condition>
                <ValueOf question="Sex" is="equal_to">M</ValueOf>
                <ResultString>
                    <TextComponent xml:lang="en">his</TextComponent>
                </ResultString>
            </Condition>
        </WordSub>
    </WordSubstitutions>
    <ModuleLogic>
        <Question name="Dog_name">
            <TextComponent xml:lang="en">
                <QuestionText>What is your dog's name?</QuestionText>
            </TextComponent>
            <ResponseType>
                <Text />
            </ResponseType>
        </Question>
        <Question name="Sex">
            <TextComponent xml:lang="en">
                <QuestionText>What is the sex of <sub ref="Dog_name" />?</QuestionText>
            </TextComponent>
            <ResponseType>
                <CodeList>
                    <Codes>
                        <CodePair code="M">
                            <TextComponent xml:lang="en">Male</TextComponent>
                        </CodePair>
                        <CodePair code="F">
                            <TextComponent xml:lang="en">Female</TextComponent>
                        </CodePair>
                    </Codes>
                </CodeList>
            </ResponseType>
        </Question>
        <Question name="Name">
            <TextComponent xml:lang="en">
                <QuestionText>What is the dogs name?</QuestionText>
            </TextComponent>
            <ResponseType>
                <Text />
            </ResponseType>
        </Question>
    </ModuleLogic>
</QuestionModule>
